{
  "name": "p5-typescript-starter",
  "version": "1.0.0",
  "description": "Project to quickly get something working in [p5.js](https://p5js.org/) and [typescript](https://www.typescriptlang.org/)",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-old": "run-p start-compile start-run",
    "start-compile": "tsc --watch",
    "start-run": "browser-sync start --server -w",
    "start": "webpack --watch && webpack-dev-server",
    "build": "webpack --config sketch/webpack.config.js --mode production",
    "deploy": "gh-pages -d dist",
    "check-types": "tsc"
  },
  "homepage": "https://github.com/Gaweph/p5-typescript-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/p5": "^0.9.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "p5": "^0.10.2"
  }
}
